{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,md}\""
  },
  "packageManager": "pnpm@8.11.0",
  "name": "dtm-database-frontend",
  "dependencies": {
    "@kuma-ui/core": "1.5.4",
    "@manypkg/cli": "0.21.1",
    "eslint": "8.53.0",
    "prettier": "3.1.0",
    "tsconfig": "workspace:*",
    "turbo": "1.10.16"
  },
  "manypkg": {
    "ignoredRules": [
      "INTERNAL_MISMATCH"
    ]
  }
}
