{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\""
  },
  "packageManager": "pnpm@8.6.10",
  "name": "dtm-database-frontend",
  "dependencies": {
    "@kuma-ui/core": "1.4.1",
    "@manypkg/cli": "^0.21.0",
    "eslint": "8.50.0",
    "prettier": "3.0.3",
    "tsconfig": "workspace:*",
    "turbo": "^1.10.14"
  },
  "manypkg": {
    "ignoredRules": [
      "INTERNAL_MISMATCH"
    ]
  }
}
